pipeline{
agent any

tools {
    maven 'maven'
  }
stages{
        stage('Build'){
                      steps{
                          script{
                                 try{
                                      sh " mvn -f pom.xml clean install"
                                 }
                                 catch(all){
                                      echo "Build Failed"
                                 }
                          }
                     }
        }
        
        
        
             }
        }

